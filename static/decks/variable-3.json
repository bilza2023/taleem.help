      "data": [
        {
          "name": "line",
          "type": "text",
          "content": "Pause for a moment and slow down.",
          "showAt": 0,
          "spItems": [
            { "type": "spImage", "content": "image.png" }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Bring attention to your breathing.",
          "showAt": 1,
          "spItems": [
            { "type": "spText", "content": "Notice how the breath moves in and out." },
            { "type": "spText", "content": "There is no need to change your breathing." },
            { "type": "spText", "content": "Let it stay natural and unforced." },
            { "type": "spText", "content": "Feel the rhythm of each breath." },
            { "type": "spText", "content": "Allow yourself to slow down." }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Become aware of your body.",
          "showAt": 2,
          "spItems": [
            { "type": "spText", "content": "Notice where your body feels relaxed." },
            { "type": "spText", "content": "Notice where your body feels tense." },
            { "type": "spText", "content": "There is no need to judge these sensations." },
            { "type": "spText", "content": "Simply observe what is present." },
            { "type": "spText", "content": "Let your body be as it is." }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Let thoughts come and go.",
          "showAt": 3,
          "spItems": [
            { "type": "spImage", "content": "image.png" }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Stay with this moment.",
          "showAt": 4,
          "spItems": [
            { "type": "spText", "content": "There is nothing you need to solve right now." },
            { "type": "spText", "content": "There is nothing you need to decide." },
            { "type": "spText", "content": "This moment does not require effort." },
            { "type": "spText", "content": "Allow yourself to simply be here." },
            { "type": "spText", "content": "This is enough for now." }
          ]
        }
      ]
    },
    {
      "type": "eq",
      "start": 10,
      "end": 20,
      "data": [
        {
          "name": "line",
          "type": "text",
          "content": "Check in with yourself.",
          "showAt": 10,
          "spItems": [
            { "type": "spImage", "content": "image.png" }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Notice how you are feeling right now.",
          "showAt": 11,
          "spItems": [
            { "type": "spText", "content": "You might feel calm or unsettled." },
            { "type": "spText", "content": "You might feel focused or distracted." },
            { "type": "spText", "content": "Any feeling that shows up is acceptable." },
            { "type": "spText", "content": "There is no correct emotion to have." },
            { "type": "spText", "content": "Just notice what is present." }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Gently name the experience.",
          "showAt": 12,
          "spItems": [
            { "type": "spText", "content": "You can name it as a feeling." },
            { "type": "spText", "content": "You can name it as a thought." },
            { "type": "spText", "content": "You can name it as a body sensation." },
            { "type": "spText", "content": "Naming helps create a little space." },
            { "type": "spText", "content": "This space makes things feel lighter." }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Allow the experience to exist.",
          "showAt": 13,
          "spItems": [
            { "type": "spImage", "content": "image.png" }
          ]
        },
        {
          "name": "line",
          "type": "text",
          "content": "Move forward gently.",
          "showAt": 14,
          "spItems": [
            { "type": "spText", "content": "You do not need to rush into action." },
            { "type": "spText", "content": "You can take the next step slowly." },
            { "type": "spText", "content": "Carry this awareness with you." },
            { "type": "spText", "content": "Let calm guide your next choice." },
            { "type": "spText", "content": "Proceed when you feel ready." }
          ]
        }
      ]
    }
